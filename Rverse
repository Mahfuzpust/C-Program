#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int a[100];
    int b[100];
    int i,j;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }

    for(i=n-1,j=0;i>=0;i--,j++)
    {

        // printf("%d ",a[i]);
        b[j]=a[i];

    }
    for(i=0;i<n;i++)
    {
         printf("%d ",b[i]);
    }
    return 0;
}
